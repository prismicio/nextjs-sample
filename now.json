{
    "version": 2,
    "name": "prismic-nextjs-sample",
    "builds": [
        { "src": "next.config.js", "use": "@now/next" }
    ],
    "routes": [
        {"src": "^/products/(?<id>[^/]*)", "dest": "/product?uid=$id"},
        {"src": "/blog", "dest": "/bloghome"},
        {"src": "^/blog/(?<id>[^/]*)", "dest": "/blogpost?uid=$id"}
    ]
}
